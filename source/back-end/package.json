  "scripts": {
    "start:dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
    "start:prod": "cross-env NODE_ENV=production node build/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test:unit": "vitest run --config ./vitest.config.ts",
    "test:unit:watch": "vitest --config ./vitest.config.ts",
    "test:int": "vitest run --config ./vitest.int.config.ts",
    "test:int:watch": "vitest --config ./vitest.int.config.ts",
    "coverage:unit": "vitest run --config ./vitest.config.ts --coverage",
    "coverage:int": "vitest run --config ./vitest.int.config.ts --coverage",
    "db:test:up": "docker compose -f ../docker-compose.dev.yaml up -d db-test",
    "db:test:down": "docker compose -f ../docker-compose.dev.yaml down -v",
    "prisma:test:push": "dotenv -e .env.test -- npx prisma db push --skip-generate"
  },
